      <display_name>Rated High Speed COP</display_name>
      <type>Double</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>4</default_value>
    </argument>
    <argument>
      <name>cop_low</name>
      <display_name>Rated Low Speed COP</display_name>
      <type>Double</type>
