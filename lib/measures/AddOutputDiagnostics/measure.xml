          <display_name>DisplayUnusedObjects</display_name>
        </choice>
        <choice>
          <value>DisplayAdvancedReportVariables</value>
          <display_name>DisplayAdvancedReportVariables</display_name>
        </choice>
        <choice>
          <value>DisplayZoneAirHeatBalanceOffBalance</value>
          <display_name>DisplayZoneAirHeatBalanceOffBalance</display_name>
        </choice>
        <choice>
          <value>DoNotMirrorDetachedShading</value>
